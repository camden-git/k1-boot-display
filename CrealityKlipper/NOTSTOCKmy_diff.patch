-    struct hd44780 *h;
-    foreach_oid(i, h, command_config_hd44780) {
-        gpio_out_write(h->rs, 0);
-        gpio_out_write(h->e, 0);
-        gpio_out_write(h->d4, 0);
-        gpio_out_write(h->d5, 0);
-        gpio_out_write(h->d6, 0);
-        gpio_out_write(h->d7, 0);
