-move_queue_push(struct move_node *m, struct move_queue_head *mh)
-{
-    m->next = NULL;
-    if (mh->first) {
-        mh->last->next = m;
-        mh->last = m;
-        return 0;
-    }
-    mh->first = mh->last = m;
-    return 1;
-}
-
