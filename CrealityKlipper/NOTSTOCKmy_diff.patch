+	uint32_t flen = 0;
+
+	uint32_t crc16Rtn = 0;
+
+	uint32_t tmpLen = 0;
+
+	uint8_t fbuff[CRC16_BUFFER_LEN] = {0};
+
+	//printf("argc = %d, argv[0] = %s, argv[1] = %s\r\n",argc, argv[0], argv[1]);	
+
+	if(argc != 2)
