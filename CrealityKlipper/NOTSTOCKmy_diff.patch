-
-// Available TCC devices
-struct tcc_info_s {
-    Tcc *tcc;
-    uint32_t pclk_id, pm_id;
-};
-static const struct tcc_info_s tcc_info[] = {
-    { TCC0, TCC0_GCLK_ID, ID_TCC0 },
-    { TCC1, TCC1_GCLK_ID, ID_TCC1 },
-    { TCC2, TCC2_GCLK_ID, ID_TCC2 },
