-// Write tx bytes to the serial port
-static void
-kick_tx(void)
-{
-    for (;;) {
-        if (!(LPC_UARTx->LSR & (1<<5))) {
-            // Output fifo full - enable tx irq
-            LPC_UARTx->IER = 0x03;
-            break;
-        }
-        uint8_t data;
-        int ret = serial_get_tx_byte(&data);
-        if (ret) {
-            // No more data to send - disable tx irq
-            LPC_UARTx->IER = 0x01;
-            break;
-        }
-        LPC_UARTx->THR = data;
-    }
-}
-
