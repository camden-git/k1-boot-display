+        max_val = max(fit_vals_t)
+        min_val = min(fit_vals_t)
+        for i in range(0, self.cfg.pi_count):
+            fit_vals_t[i] = (fit_vals_t[i] - min_val) / (max_val - min_val)
+        for i in range(0, self.cfg.pi_count - 1):
