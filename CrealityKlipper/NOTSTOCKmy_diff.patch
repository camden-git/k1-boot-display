-process_io(void)
-{
-    for (;;) {
-        CT_INTC.SECR0 = ((1 << KICK_PRU0_FROM_ARM_EVENT)
-                         | (1 << KICK_PRU0_EVENT));
-        check_can_send();
-        int can_sleep = check_can_read();
-        if (can_sleep) {
-            flush_messages();
-            while (!(read_r31() & (1 << (WAKE_PRU0_IRQ + R31_IRQ_OFFSET)))) {
-                //asm("slp 1");
-            }
-        }
-    }
-}
