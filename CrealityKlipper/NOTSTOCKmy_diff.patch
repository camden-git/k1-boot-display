-void
-gpio_out_toggle_noirq(struct gpio_out g)
-{
-    GPIO_TypeDef *regs = g.regs;
-    regs->ODR ^= g.bit;
-}
-
-void
-gpio_out_toggle(struct gpio_out g)
-{
-    irqstatus_t flag = irq_save();
-    gpio_out_toggle_noirq(g);
-    irq_restore(flag);
-}
