-    uint8_t regs[] = {AR_FIFO_COUNT_H};
-    uint8_t msg[2];
-    i2c_read(mp->i2c->i2c_config, sizeof(regs), regs, 2, msg);
-    msg[0] = 0x1F & msg[0]; // discard 3 MSB per datasheet
-    return (((uint16_t)msg[0]) << 8 | msg[1]);
