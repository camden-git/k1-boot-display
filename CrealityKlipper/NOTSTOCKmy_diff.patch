+uint16_t crc16(const uint8_t *buf, uint32_t len, uint16_t crc) 
+{
+    uint32_t counter;
+    
+	for (counter = 0; counter < len; counter++)
+	{
+		crc = (crc << 8) ^ crc16tab[((crc >> 8) ^ *buf++) & 0x00FF];
+	}
+
+    return crc;
+}
