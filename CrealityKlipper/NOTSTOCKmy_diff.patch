-static void
-init_pins(uint32_t bus)
-{
-    const struct spi_info *si = &spi_bus[bus];
-    gpio_peripheral(si->sck_pin, si->sck_periph, 0);
-    gpio_peripheral(si->miso_pin, si->rx_periph, 1);
