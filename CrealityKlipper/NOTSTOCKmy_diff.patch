-            UDP->UDP_GLB_STAT |= UDP_GLB_STAT_FADDEN;
-            set_address = 0;
-        }
-    }
-    if (s & (1<<USB_CDC_EP_BULK_OUT))
-        usb_notify_bulk_out();
-    if (s & (1<<USB_CDC_EP_BULK_IN))
-        usb_notify_bulk_in();
