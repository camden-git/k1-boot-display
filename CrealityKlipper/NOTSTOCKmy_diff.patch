-    struct gpio_out sclk, sid;
-};
-
-
-/****************************************************************
- * Transmit functions
- ****************************************************************/
-
-static uint32_t
-nsecs_to_ticks(uint32_t ns)
-{
-    return timer_from_us(ns * 1000) / 1000000;
-}
-
-static inline void
-ndelay(uint32_t nsecs)
-{
-    if (CONFIG_MACH_AVR)
-        // Slower MCUs don't require a delay
-        return;
-    uint32_t end = timer_read_time() + nsecs_to_ticks(nsecs);
