-send_pru1_command(const struct command_parser *cp)
-{
-    barrier();
-    SHARED_MEM->next_command = cp;
-    barrier();
-    write_r31(R31_WRITE_IRQ_SELECT | (KICK_PRU1_EVENT - R31_WRITE_IRQ_OFFSET));
-}
