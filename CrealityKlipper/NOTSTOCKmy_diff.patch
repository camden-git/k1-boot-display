+READ_SIZE = 512 * 1024
+SUPPORTED_SLICERS: List[Type[BaseSlicer]] = [
+    PrusaSlicer, Slic3rPE, Slic3r, Cura, Simplify3D,
+    KISSlicer, IdeaMaker, IceSL, KiriMoto, Creality
+]
+SUPPORTED_DATA = [
+    'gcode_start_byte',
+    'gcode_end_byte',
+    'layer_count',
+    'object_height',
+    'estimated_time',
+    'nozzle_diameter',
+    'layer_height',
+    'first_layer_height',
+    'first_layer_extr_temp',
+    'first_layer_bed_temp',
+    'chamber_temp',
+    'filament_name',
+    'filament_type',
+    'filament_total',
+    'filament_weight_total',
+    'model_info']
