-static void
-i2c_stop(I2C_TypeDef *i2c, uint32_t timeout)
-{
-    i2c->CR1 = I2C_CR1_STOP | I2C_CR1_PE;
-    i2c_wait(i2c, 0, I2C_SR1_TXE, timeout);
-}
