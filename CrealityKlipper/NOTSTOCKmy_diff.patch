-    ret = set_non_blocking(mfd);
-    if (ret)
-        return -1;
-    ret = set_close_on_exec(mfd);
-    if (ret)
-        return -1;
-    ret = set_close_on_exec(sfd);
-    if (ret)
-        return -1;
