-static void
-set_pin_mux(uint8_t pin, uint8_t val)
-{
-    return; // XXX - can not set mux value from PRU
-
-    uint8_t mux_offset = gpio_mux_offset[pin];
-    if (mux_offset == 0xff)
-        shutdown("Invalid mux pin");
-    *MUXREG(mux_offset) = val;
