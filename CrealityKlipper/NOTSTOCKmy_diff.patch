-    struct spidev_s *spi = spidev_oid_lookup(oid);
-    uint8_t data_len = args[1];
-    uint8_t *data = command_decode_ptr(args[2]);
-    spidev_transfer(spi, 1, data_len, data);
-    sendf("spi_transfer_response oid=%c response=%*s", oid, data_len, data);
