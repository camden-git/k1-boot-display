-                  | (addr << USB_OTG_DCFG_DAD_Pos));
-    usb_send_ep0(NULL, 0);
-    usb_notify_ep0();
-}
-
-void
-usb_set_configure(void)
-{
-    usb_irq_disable();
-    // Configure and enable USB_CDC_EP_ACM
-    USB_OTG_INEndpointTypeDef *epi = EPIN(USB_CDC_EP_ACM);
-    epi->DIEPTSIZ = (USB_CDC_EP_ACM_SIZE
-                     | (1 << USB_OTG_DIEPTSIZ_PKTCNT_Pos));
