-// Convert a register and bit location back to an integer pin identifier
-static int
-regs_to_pin(GPIO_TypeDef *regs, uint32_t bit)
-{
-    int i;
-    for (i=0; i<ARRAY_SIZE(digital_regs); i++)
-        if (digital_regs[i] == regs)
-            return GPIO('A' + i, ffs(bit)-1);
-    return 0;
-}
