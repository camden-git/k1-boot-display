-void
-spi_transfer(struct spi_config config, uint8_t receive_data
-             , uint8_t len, uint8_t *data)
-{
-    LPC_SSP_TypeDef *spi = config.spi;
-    if (receive_data) {
-        while (len--) {
-            spi->DR = *data;
-            // wait for read data to be ready
-            while (!(spi->SR & (1<<2)))
-                ;
-            // get data
-            *data++ = spi->DR;
-        }
-    } else {
-        while (len--) {
-            spi->DR = *data++;
-            // wait for read data to be ready
-            while (!(spi->SR & (1<<2)))
-                ;
-            // read data (to clear receive fifo)
-            spi->DR;
-        }
-    }
-}
