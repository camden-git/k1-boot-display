-                                  AFIO_MAPR_SWJ_CFG_DISABLE);
-    else
-        stm32f1_alternative_remap(AFIO_MAPR_SWJ_CFG_Msk,
-                                  AFIO_MAPR_SWJ_CFG_JTAGDISABLE);
-
-    sched_main();
-}
