-void
-gpio_out_write(struct gpio_out g, uint32_t val)
-{
-    GPIO_TypeDef *regs = g.regs;
-    if (val)
-        regs->BSRR = g.bit;
-    else
-        regs->BSRR = g.bit << 16;
-}
-
-
-struct gpio_in
-gpio_in_setup(uint32_t pin, int32_t pull_up)
-{
-    if (!gpio_valid(pin))
